{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/"
    },
    {
      "src": "products",
      "dest": "server.js"
    },
    {
      "src": "products/(.*)",
      "dest": "server.js"
    }
  ]
}
